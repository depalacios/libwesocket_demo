{
   "configurations": [
        {
            "name": "GDB Remoto Yocto",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/main", 
            "cwd": "${workspaceFolder}",
            "targetArchitecture": "arm64",
            "miDebuggerPath": "/opt/fsl-imx-xwayland/6.12-walnascar/sysroots/x86_64-pokysdk-linux/usr/bin/aarch64-poky-linux/aarch64-poky-linux-gdb", 
            "miDebuggerServerAddress": "${config:myRemoteIpAddr}:2345", 
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set sysroot for remote debugging",
                    "text": "set sysroot /opt/fsl-imx-xwayland/6.12-walnascar/sysroots/armv8a-poky-linux", 
                    "ignoreFailures": false
                }
            ],
        }
    ]
}